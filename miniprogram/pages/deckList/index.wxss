@import "../../style/base.wxss";
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}

.occupation-bar{
    width: 100%;
    padding: 20rpx;
    box-sizing: border-box;
    display: flex;
    border-bottom:1rpx solid #ccc;
    box-shadow: 0 1px 2px 0 #b9b9b9;
    z-index: 1;
    justify-content: space-between;
}
.occupation-bar image{
    width: 60rpx;
    height: 60rpx;
    padding: 5rpx;
}
.occupation-bar .active{
    background:#2d8cf0;
    border-radius:50%;
}