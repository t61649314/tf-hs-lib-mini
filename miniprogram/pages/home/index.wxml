<view class="container" style="height:{{componentHeight}}px">
    <div class="navigator-box">
        <text class="box-title">狂野</text>
        <navigator wx:for="{{wildFromList}}" wx:key="{{index}}" url="../reportList/index?type=wild&from={{item.from}}&cnName={{item.cnName}}" open-type="navigate" class="item">
            <image class="type-img" src="../../images/wild-icon.png"></image>
            <text class="title">{{item.cnName}}</text>
            <span class="new-icon" wx:if="{{item.hasNew}}">new!</span>
        </navigator>
    </div>
    <div class="navigator-box">
        <text class="box-title">标准</text>
        <navigator wx:for="{{standardFromList}}" wx:key="{{index}}" url="../reportList/index?type=standard&from={{item.from}}&cnName={{item.cnName}}" open-type="navigate" class="item">
            <image class="type-img" src="../../images/standard-icon.png"></image>
            <text class="title">{{item.cnName}}</text>
            <span class="new-icon" wx:if="{{item.hasNew}}">new!</span>
        </navigator>
    </div>
    <ad class="my-ad" unit-id="adunit-5215dd089956d3e6"></ad>
</view>